{
  "name": "embeddable.com-boilerplate-repo",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "ct": "tsconfig --noEmit",
    "test": "jest",
    "cube:playground": "docker compose -f ./src/cube-playground.yml up",
    "cube:playground:update": "docker pull cubejs/cube:latest",
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' embeddable dev",
    "embeddable:build": "embeddable build",
    "embeddable:dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' embeddable dev",
    "embeddable:login": "embeddable login",
    "embeddable:push": "embeddable push",
    "embeddable:upgrade": "npx npm-check-updates -u \"/embeddable/\"",
    "embedding-preview": "node src/scripts/embedding-preview.cjs"
  },
  "dependencies": {
    "@embeddable.com/vanilla-components": "^1.1.15",
    "@stencil/core": "^4.23.0",
    "@types/jest": "^30.0.0",
    "chart.js": "^4.4.9",
    "dotenv": "^17.2.3",
    "jest": "^30.2.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.4.5"
  },
  "devDependencies": {
    "@babel/runtime": "^7.28.4",
    "@cubejs-client/core": "^1.5.1",
    "@embeddable.com/core": "^2.12.0",
    "@embeddable.com/react": "^2.11.0",
    "@embeddable.com/sdk-core": "^4.1.2",
    "@embeddable.com/sdk-react": "^4.0.6",
    "@types/node": "^24.10.0",
    "cross-env": "^7.0.3"
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af",
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js"
    ]
  }
}
